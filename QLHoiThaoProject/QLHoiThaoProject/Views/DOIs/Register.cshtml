@model QLHoiThaoProject.Models.EFModel.DOI
<div class="container-fluid f">
    <div class="container f">
        <div class="panel panel-primary">
            <div class="row">
                <div class="col-md-9">
                    <div id="bookbody" style="width:1106px;">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="panel panel-primary">
                                    <div class="panel-heading">ĐĂNG KÝ ĐỘI THI ĐẤU</div>
                                    <div class="panel-body" style="padding-bottom: 0px;">
                                        <div class="row">
                                            @using (Html.BeginForm())
                                            {
                                                @Html.AntiForgeryToken()

                                                <div class="form-horizontal">

                                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                                    <div class="form-group">
                                                        @Html.LabelFor(model => model.IDHT, "IDHT", htmlAttributes: new { @class = "control-label col-md-3" })
                                                        <div class="col-md-6">
                                                            @Html.DropDownList("IDHT", null, htmlAttributes: new { @class = "form-control" })
                                                            @Html.ValidationMessageFor(model => model.IDHT, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        @Html.LabelFor(model => model.IDMTD, "IDMTD", htmlAttributes: new { @class = "control-label col-md-3" })
                                                        <div class="col-md-6">
                                                            @Html.DropDownList("IDMTD", null, htmlAttributes: new { @class = "form-control" })
                                                            @Html.ValidationMessageFor(model => model.IDMTD, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                    @{
                                                        var session = (QLHoiThaoProject.Common.TVLogin)Session[QLHoiThaoProject.Common.TVCommonConstants.TVUSER_SESSION];
                                                    }
                                                    <div class="form-group">
                                                        <label class="control-label col-md-3">LOP</label>
                                                        @*@Html.LabelFor(model => model.IDLOP, "IDLOP", htmlAttributes: new { @class = "control-label col-md-2" })*@
                                                        <div class="col-md-6">
                                                            <input type="text" value="@session.IDLOP" name="IDLOP" class="form-control" disabled />
                                                            @*@Html.DropDownList("IDLOP", null, htmlAttributes: new { @class = "form-control" })
                                                                @Html.ValidationMessageFor(model => model.IDLOP, "", new { @class = "text-danger" })*@
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        @Html.LabelFor(model => model.TENDOI, htmlAttributes: new { @class = "control-label col-md-3" })
                                                        <div class="col-md-6">
                                                            @Html.EditorFor(model => model.TENDOI, new { htmlAttributes = new { @class = "form-control" } })
                                                            @Html.ValidationMessageFor(model => model.TENDOI, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>

                                                    <div class="form-group">
                                                        <div class="col-md-offset-3 col-md-6">
                                                            <button class="btn" href="@Url.Action("Index","Home")">Hủy</button>
                                                            <input type="submit" value="Create" class="btn btn-default" />
                                                        </div>
                                                    </div>
                                                </div>
                                                        }
</div></div></div></div></div></div></div></div></div></div></div>