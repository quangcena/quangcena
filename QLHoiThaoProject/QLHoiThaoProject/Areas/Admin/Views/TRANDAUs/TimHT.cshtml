@model IEnumerable< QLHoiThaoProject.Models.EFModel.DOI>

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@section IndexAdmin{
    Quản lý trận đấu
}


<div class="row">
    <ol class="breadcrumb">
        <li class="active">Chọn kì hội thao</li>
    </ol>
</div><!--/.row-->


<div>

    <table class="table table-hover">
        <tr>
            <th>
                Mã hội thao 
            </th>
            <th>
                Tên hội thao
            </th>
            
            <th></th>
        </tr>
        @foreach (var item in ViewBag.IDHT)
        {
            <tr>
                <td>
                    @item.IDHT
                </td>
                <td>
                    @item.TENHT
                </td>                               
                <td>
                    @Html.ActionLink("Chọn", "TimMTD", new { id = item.IDHT })
                </td>
            </tr>
        }
    </table>      
</div>
